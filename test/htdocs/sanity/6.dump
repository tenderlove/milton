<script language="javascript" type="text/javascript">

//Log#: 051735
var intCharLength = 0;
var strEvtType = "";
var btnAry = new Array("lnkClockIn", "lnkClockOut", "lnkLunchOut", "lnkTransfer", "lnkTimeSheet", "lnkSupplementalEarnings");
for(i=0; i<btnAry.length; i++)
{
	var btn = document.getElementById("UCTodaysActivities_" + btnAry[i]);
		if(btn)
		{
			intCharLength += btn.innerHTML.length;
			
		}
}
if(intCharLength > 80)
{
	document.getElementById("sd1").className = "SectionDivider";
	document.getElementById("sd2").className = "HiddenItem";
}
//This function is called once the call to the server returns.
function Employee_CallBack(response)
{
	document.getElementById("rptTable").className = "HiddenItem";
	
	var ActDiv = document.getElementById("divActivities");
	var objAxActMR = new AjaxActivityMethodResult();
	var ActCount = 0;
	var strTimeCardApprovalFlag = "F";	
	
	objAxActMR.populateFromXml(response.value);
	
	if (strEvtType == "IN")
	{
		UCTodaysActivities_lnkClockIn.disabled = false;
	}
	else if (strEvtType == "OUT")
	{
		UCTodaysActivities_lnkClockOut.disabled = false;
	}
	else if (strEvtType == "LUNCH")
	{
		UCTodaysActivities_lnkLunchOut.disabled = false;
	}
	
	if (objAxActMR.status == "0")
	{
		if (strEvtType == "IN")
		{
			DisplayMessage("StatusMessage",'Clock In Successful.');
		}
		else if (strEvtType == "OUT")
		{
			DisplayMessage("StatusMessage",'Clock Out Successful.');
		}
		else if (strEvtType == "LUNCH")
		{
			DisplayMessage("StatusMessage",'Lunch Out Successful');
		}
		
		if (objAxActMR.colEvt.size() > 0)
		{						
			var strTable = "<table cellSpacing=\"0\" cellPadding=\"4\" width=\"100%\" border=\"0\">";
			
			strTimeCardApprovalFlag = objAxActMR.TimeCardAppFlag;
			
			for(ActCount = 0; ActCount < objAxActMR.colEvt.size(); ActCount++)
			{
				if (ActCount % 2 == 0)
				{
					strTable = strTable + "<tr class=DataTable-BodyRow-Odd>"; 	
				}
				else
				{
					strTable = strTable + "<tr class=DataTable-BodyRow-Even>"; 
				}
				
				strTable = strTable + "<td>" + objAxActMR.colEvt.get(ActCount).EvtDesc + "</td>";
				strTable = strTable + "<td>" + objAxActMR.colEvt.get(ActCount).EvtTime + "</td>";
				
				if  (objAxActMR.colEvt.get(ActCount).TimeDesc != null)
				{
					strTable = strTable + "<td>" + objAxActMR.colEvt.get(ActCount).TimeDesc + "</td>";
				}
				else
				{
					strTable = strTable + "<td></td>";
				}
				
				strTable = strTable + "</tr>"; 
			}
			
			strTable = strTable + "</table>";
			
			document.getElementById("divActivities").className = "";
			document.getElementById("divActivities").innerHTML = strTable;
			
			if (strTimeCardApprovalFlag == 'R')
			{
				if (document.getElementById("UCTodaysActivities_hypTimeCardApproval") == null)
				{
					document.getElementById("divhypTimeCardApproval").className = "HiddenItem";
					document.getElementById("hypTCApproval").innerHTML = "<a href=" + '/ezLaborManagerNet/EmployeeServices/Labor/TimecardApproval.aspx?sID=93e9a56d648c4eb2b068e1e0141caaa4&eid=1000002506&tcCurrState=C' + ">" + 
												'Timecard Approval Has Been Cleared' + "</a>";
				}
			}
			else
			{
				document.getElementById("divhypTimeCardApproval").className = "HiddenItem";
				document.getElementById("hypTCApproval").innerHTML = "";
			}
			
		}
		else
		{
			document.getElementById("divActivities").className = "InfoText";
			document.getElementById("divActivities").innerHTML = 'No Activities';
		}
	}
	else
	{
		DisplayMessage("ErrorText", objAxActMR.message);
	}	
}


//Displays successful message or error message.
function DisplayMessage(strclassname, strMessage)
{
	document.getElementById("divlitmessage").className = strclassname;
	document.getElementById("divlitmessage").innerHTML = strMessage;

}

function ClockEmp(timeEvtType)
{
	var strConfirm = "";
	strEvtType = timeEvtType;
	
	if (timeEvtType == "IN")
	{
		strConfirm = 'Click OK to Clock In.' 
	}
	else if (timeEvtType == "OUT")
	{
		strConfirm = 'Click OK to Clock Out.' 
	}
	else if (timeEvtType == "LUNCH")
	{
		strConfirm = 'Click OK to Lunch Out.' 
	}
	
	var blnconfirm = confirm(strConfirm);
	
	if (blnconfirm)
	{
		document.getElementById("litMsg").innerHTML = "";
		
		ClockAction.SessionID = "93e9a56d648c4eb2b068e1e0141caaa4";
		
		if (timeEvtType == "IN")
		{
			UCTodaysActivities_lnkClockIn.disabled = true;
			ClockAction.ClockFunction = "IN";
		}
		else if(timeEvtType == "OUT")
		{
			UCTodaysActivities_lnkClockOut.disabled = true;
			ClockAction.ClockFunction = "OUT";
		}
		else if(timeEvtType == "LUNCH")
		{
			UCTodaysActivities_lnkLunchOut.disabled = true;
			ClockAction.ClockFunction = "LUNCH";
		}
		
		//calls the sever function response returns to Employee_Callback function
		ClockFunctionProvider.PerformClockFunction(ClockAction.prototype.toXmlString(), Employee_CallBack);	
	}	
	return false;
}

</script>

